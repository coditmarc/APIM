    }
  },
  "resources": [
    {
      "type": "Microsoft.ApiManagement/service/subscriptions",
      "name": "[concat(parameters('APIManagementInstanceName'), '/B9508F6AA4544741A8DDD8DC889494C4')]",
      "apiVersion": "2020-12-01"  ,
      "scale": null,
      "properties": {
        "userId": "[resourceId('Microsoft.ApiManagement/service/users', parameters('APIManagementInstanceName'), 'codit-care-codit-eu')]",
        "productId": "[resourceId('Microsoft.ApiManagement/service/products', parameters('APIManagementInstanceName'), 'Support')]",
        "scope": "[concat(resourceId('Microsoft.ApiManagement/service/products', parameters('APIManagementInstanceName')), 'starter')]",
